def Xform "World"
{
    over "base"
    {
        over "base_mesh"
        {
            rel material:binding = </World/Looks/common/white> (
                bindMaterialAs = "weakerThanDescendants"
            )

            over "base_white"
            {
                rel material:binding = </World/Looks/common/white> (
                    bindMaterialAs = "weakerThanDescendants"
                )
            }

            over "base_black"
            {
                rel material:binding = </World/Looks/common/black_textured> (
                    bindMaterialAs = "weakerThanDescendants"
                )
            }
        }
    }

    over "tower"
    {
        over "tower_mesh"
        {
            rel material:binding = </World/Looks/common/white> (
                bindMaterialAs = "weakerThanDescendants"
            )
        }
    }

    over "arm_link_1"
    {
        over "arm_link_1_mesh"
        {
            rel material:binding = </World/Looks/common/white> (
                bindMaterialAs = "weakerThanDescendants"
            )
        }
    }

    over "arm_link_2"
    {
        over "arm_link_2_mesh"
        {
            rel material:binding = </World/Looks/common/white> (
                bindMaterialAs = "weakerThanDescendants"
            )
        }
    }

    over "gripper"
    {
        over "gripper_mesh"
        {
            rel material:binding = </World/Looks/common/metal> (
                bindMaterialAs = "weakerThanDescendants"
            )
        }
    }

    over "finger_right"
    {
        over "finger_right_mesh"
        {
            rel material:binding = </World/Looks/common/black> (
                bindMaterialAs = "weakerThanDescendants"
            )
        }
    }

    over "finger_left"
    {
        over "finger_left_mesh"
        {
            rel material:binding = </World/Looks/common/black> (
                bindMaterialAs = "weakerThanDescendants"
            )
        }
    }

    over "Looks"
    {
        over "common"
        {
            token visibility = "inherited"
        }
    }
}

def Xform "Environment"
{
    quatd xformOp:orient = (1, 0, 0, 0)
    double3 xformOp:scale = (1, 1, 1)
    double3 xformOp:translate = (0, 0, 0)
    uniform token[] xformOpOrder = ["xformOp:translate", "xformOp:orient", "xformOp:scale"]

    def DistantLight "defaultLight" (
        prepend apiSchemas = ["ShapingAPI"]
    )
    {
        float angle = 1
